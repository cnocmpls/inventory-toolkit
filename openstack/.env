# .env for OpenStack
DATACENTER_NAME="yourdcname"
OS_AUTH_URL="http://your_keystone_ip_or_hostname:5000/v3"
OS_PROJECT_NAME="your_project_name"
OS_PROJECT_DOMAIN_NAME="Default" # Or your specific project domain
OS_USERNAME="your_username"
OS_PASSWORD="your_password"
OS_USER_DOMAIN_NAME="Default" # Or your specific user domain
OS_REGION_NAME="your_region_name" # e.g., RegionOne

# Optional: For SSL verification with custom CA certificates
# OS_CACERT="/path/to/your/custom_ca_bundle.pem"

# Optional: To disable SSL verification (NOT RECOMMENDED FOR PRODUCTION)
# OS_VERIFY_SSL="false"
# or OS_INSECURE="true" (openstacksdk uses this)
